# . $HOME/.shellrc.load

export GRADLE_HOME="/opt/gradle-1.12"
export GRADLE_OPTS="-Xms128m -Xmx1024m -Xss8m -XX:MaxPermSize=512m"

export GOPATH=$HOME/go

export LESS='-R -i'

export PATH=$PATH:$GOPATH/bin:$GRADLE_HOME/bin:$HOME/bin


mko() { mkdir "$@" && cd "$_"; }
er() { /usr/bin/dtrx -o "$@" && rm -f "$_"; }

